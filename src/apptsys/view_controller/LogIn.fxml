<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="250.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apptsys.view_controller.LogInController">
   <children>
      <Label fx:id="mainLabel" alignment="CENTER" contentDisplay="BOTTOM" layoutX="119.0" layoutY="14.0" prefHeight="17.0" prefWidth="362.0" text="APPOINTMENT SCHEDULING SYSTEM" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="connectingFromLabel" alignment="CENTER" contentDisplay="BOTTOM" layoutX="119.0" layoutY="50.0" prefHeight="17.0" prefWidth="362.0" text="Client connecting from:" textAlignment="CENTER">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="userIDField" alignment="CENTER" layoutX="181.0" layoutY="99.0" onKeyReleased="#onUserIDPressed" prefHeight="25.0" prefWidth="239.0" promptText="USER ID" />
      <TextField fx:id="passwordField" alignment="CENTER" layoutX="181.0" layoutY="130.0" onKeyReleased="#onPassPressed" prefHeight="25.0" prefWidth="239.0" promptText="PASSWORD" />
      <Button fx:id="logInButton" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" layoutX="256.0" layoutY="164.0" mnemonicParsing="false" onAction="#onLogInButton" prefHeight="25.0" prefWidth="90.0" text="CONNECT" textAlignment="CENTER" />
   </children>
</AnchorPane>
